generator client {
  provider        = "prisma-client-js" // Generates the Prisma Client
  previewFeatures = ["driverAdapters", "prismaSchemaFolder"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL_LOCAL")
}


enum Category {
    Electronics
    Cameras
    Laptops
    Accessories
    Headphones
    Food
    Books
    Clothes_Shoes
    Beauty_Health
    Sports
    Outdoor
    Home
}

enum ProductionTypeEnum {
    Sell
    Rent
}