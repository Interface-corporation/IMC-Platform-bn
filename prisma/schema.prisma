generator client {
  provider = "prisma-client-js" // Generates the Prisma Client
  previewFeatures = ["driverAdapters", "omitApi"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String     @id @default(uuid())
  email     String     @unique
  name      String
  password  String?
  createdAt DateTime @default(now())
}
